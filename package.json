{
  "name": "calculator-app",
  "version": "1.0.0",
  "description": "A simple calculator application",
  "main": "calculator.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --testResultsProcessor=\"jest-junit\"",
    "start": "node calculator.js"
  },
  "keywords": ["calculator", "nodejs"],
  "author": "DevOps Team",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.0.0",
    "jest-html-reporter": "^3.0.0",
    "jest-junit": "^16.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "html",
      "lcov"
    ],
    "testResultsProcessor": "jest-junit",
    "reporters": [
      "default",
      "jest-html-reporter"
    ],
    "verbose": true
  }
}